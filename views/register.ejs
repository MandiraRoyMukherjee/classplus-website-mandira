<body>

<div class="container">
  <form method="POST" action="/register">
    <h2>Registration Form </h2>
    <input name="name" type="text" id="name" class="form-control" placeholder="Full Name"
              autofocus="true" value="<%= qs.person %>"/>
    <input name="password" id="password" type="password" class="form-control" placeholder="Password" value="<%= qs.person %>"/>

    <input name="email" id="email" type="text" class="form-control" placeholder="Email Address"
              autofocus="true" value="<%= qs.person %>"/>
    <!-- <input name="roles" type="text" class="form-control" placeholder="roles" value="<%= qs.person %>"/> -->
    <input name="class" type="text" id="class" class="form-control" placeholder="Class" value="<%= qs.class %>"/>
    <input name="parentPhoneNo" id="parentPhoneNo" type="text" class="form-control" placeholder="Parent Phone Number" value="<%= qs.parentPhoneNo %>"/>
    <button class="btn btn-lg btn-primary btn-block" id="submit" onclick="submit()" type="submit" value="submit">Register</button>
    <!-- <button class="btn btn-lg btn-primary btn-block" type="submit" value="submit" href='/login'>Register</button> -->
    <div style="margin-bottom:10px;margin-top:15px"> already have a account? <a href='/login'>Login</a></div>

</form>
