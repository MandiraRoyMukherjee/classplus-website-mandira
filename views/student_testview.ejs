                                                      <%}%>  
                                                      <br>
                                                      <ul class='list-group mt-4 mb-4'>
                                                    
                              
                                                          <li class="answer list-group-item ">
                                                             1. <label><%= questions[i].optionA%> </label>
                                                          </li>
                                                          <li class="answer list-group-item ">
                                                             2. <label><%= questions[i].optionB%> </label>
                                                          </li>
                                                          <li class="answer list-group-item ">
                                                            3. <label> <%= questions[i].optionC%> </label>
                                                          </li>
                                                          <li class="answer list-group-item" >
                                                            4. <label><%= questions[i].optionD%> </label>
                                                          </li>
                                                          
    
                                                      </ul>
                                                      <h6>Correct answer option no: <%=questions[i].answer%></h6>
